{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "target": "es6",
    "lib": ["ES2015", "DOM"],
		"allowJs": true,
		"noEmit": true,
    "module": "ESNext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
    "jsx": "react-jsx",
		"incremental": true,
  },
  "display": "React Library",
  "extends": "./base.json",
}
